.PHONY: assemble clean edit html update update-delete

doc-assemble: doc/ndesk-options-docs.zip doc/ndesk-options-docs.tree

$(srcdir)/doc/en/examples/NDesk.Options.dll: $(srcdir)/lib/ndesk-options/NDesk.Options.dll
	cp $^ $@
	-cp $^.mdb $@.mdb

%.exe: %.cs $(srcdir)/doc/en/examples/NDesk.Options.dll
	gmcs -debug+ -r:Mono.Posix.dll -r:System.Core.dll -lib:doc/en/examples -r:NDesk.Options.dll -out:$@ $<

$(srcdir)/doc/en/examples/locale/es/LC_MESSAGES/localization.mo: $(srcdir)/doc/en/examples/localization-es.po
	msgfmt $< -o $@

$(srcdir)/doc/en/examples/localization.exe: $(srcdir)/doc/en/examples/locale/es/LC_MESSAGES/localization.mo

%.txt: %.in %.exe
	-rm $@
	exec 3<$< ; \
	while read line 0<&3 ; do \
		if test -n "$$line" ; then \
			echo "$$ $$line" | sed 's#doc/en/examples/##' >> $@ ; \
			sh -c "$$line" >> $@ ; \
		else \
			echo "" >> $@ ; \
		fi ; \
	done ; \
	exec 3>&-;

doc/ndesk-options-docs.zip doc/ndesk-options-docs.tree: $(DOCUMENTATION)
	-mkdir doc
	$(MDASSEMBLER) --ecma $(srcdir)/doc/en -o doc/ndesk-options-docs

doc-update: $(addprefix lib/ndesk-options/, $(ASSEMBLIES)) $(srcdir)/doc/en/examples/*.cs $(DOC_EXAMPLES_OUTPUT)
	for a in $(ASSEMBLIES) ; do \
		$(UPDATER) -assembly:lib/ndesk-options/$$a ; \
	done

doc-update-delete: $(addprefix lib/ndesk-options/, $(ASSEMBLIES)) $(srcdir)/doc/en/examples/*.cs $(DOC_EXAMPLES_OUTPUT)
	for a in $(ASSEMBLIES) ; do \
		$(UPDATER) --delete -assembly:lib/ndesk-options/$$a ; \
	done

doc-edit:
	monodoc --edit $(srcdir)/doc/en

doc-html: $(DOCUMENTATION)
	mdoc export-html --default-template > doc/doc-def-template.xsl
	mdoc export-html --template=doc/doc-template.xsl --ext=html -o doc/$(PACKAGE) doc/en
	cp doc/prettyprint.js doc/$(PACKAGE)
	(cd doc && zip -q -r ../archive/$(PACKAGE)-$(VERSION)-docs.zip $(PACKAGE))

clean:
	-rm $(CLEANFILES)
	-rm -Rf $(PACKAGE)-$(VERSION)

.PHONY: all check rpm

all: $(nolibdir)/NDesk.Options.dll

dist-hook:
	if [ -d $(srcdir)/.git ] ; then \
		(cd $(srcdir) && git-log > $(distdir)/ChangeLog) ; \
	fi

$(nolibdir)/NDesk.Options.dll: $(OPTIONS_SRC)
	-mkdir -p `dirname $@`
	$(CSC) -debug+ $(OPTIONS_SRC) -langversion:ISO-2 -t:library -out:$@

$(nolibdir)/NDesk.Options_test.dll: $(nolibdir)/NDesk.Options.dll $(OPTIONS_TEST_SRC)
	-mkdir -p `dirname $@`
	$(CSC) -debug+ -t:library -out:$@ -r:$(nolibdir)/NDesk.Options.dll \
		-r:nunit.core.dll -r:nunit.framework.dll -r:nunit.util.dll \
		$(OPTIONS_TEST_SRC) 

$(nolibdir)/NDesk.Options_test_linq.dll: $(OPTIONS_SRC) $(OPTIONS_TEST_SRC)
	-mkdir -p `dirname $@`
	$(CSC) -debug+ -d:LINQ -t:library -out:$@ \
		-r:nunit.core.dll -r:nunit.framework.dll -r:nunit.util.dll \
		$(OPTIONS_SRC) $(OPTIONS_TEST_SRC) 

check: $(nolibdir)/NDesk.Options_test.dll $(nolibdir)/NDesk.Options_test_linq.dll
	nunit-console2 $(nolibdir)/NDesk.Options_test.dll
	nunit-console2 $(nolibdir)/NDesk.Options_test_linq.dll

rpm:
	tgz=$(DIST_ARCHIVES); \
	if [ ! -e $(DIST_ARCHIVES) ] ; then tgz=archive/$(DIST_ARCHIVES) ; fi ; \
	cp $$tgz /usr/src/packages/SOURCES/$(DIST_ARCHIVES) && \
		rpmbuild -bb ndesk-options.spec && \
		cp /usr/src/packages/RPMS/noarch/$(distdir)*.rpm archive

bin-zip:
	-rm archive/$(distdir).bin.zip
	$(MAKE) prefix= DESTDIR=$(distdir).bin install && \
		zip -r archive/$(distdir).bin.zip $(distdir).bin && \
		rm -Rf $(distdir).bin

prepare-release:
	$(MAKE) distcheck
	$(MAKE) bin-zip
	$(MAKE) doc-html
	mv $(DIST_ARCHIVES) archive
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
